        "@nestjs/event-emitter": "1.3.0",
        "@nestjs/graphql": "10.0.21",
        "@nestjs/jwt": "9.0.0",
        "@nestjs/mapped-types": "*",
        "@nestjs/mercurius": "10.0.21",
        "@nestjs/microservices": "9.0.8",
        "@nestjs/platform-fastify": "9.1.5",
        "@nestjs/terminus": "9.1.1",
        "@prisma/client": "4.2.1",
