        "@nestjs/mercurius": "10.0.16",
        "@nestjs/passport": "8.2.2",
        "@nestjs/platform-fastify": "8.4.7",
        "@nestjs/schedule": "2.0.1",
        "@prisma/client": "^4.1.0",
        "@types/cron": "2.0.0",
        "argon2": "0.28.5",
        "dotenv": "16.0.1",
        "envalid": "7.3.1",
        "got": "12.1.0",
        "graphql": "16.5.0",
        "graphql-request": "4.3.0",
